{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard-with-typescript",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslit",
    "prettier/standard",
    "prettier/react"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "typescript-eslint"],
  "rules": {
    "semi": "off",
    "prettier/prettier": "error",
    "react/prop-types": "off"
  }
}

